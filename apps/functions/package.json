{
  "name": "@gentlepeople/taleus-functions",
  "private": true,
  "version": "0.0.1",
  "main": "dist/src/index.js",
  "scripts": {
    "prebuild": "pnpm rimraf dist/",
    "schema:build": "cd ../schema && pnpm prisma generate && pnpm tsc && cp -R ./generated ./dist/",
    "schema:build-no-engine": "PRISMA_NO_ENGINE=true cd ../schema && pnpm prisma generate --no-engine && pnpm tsc && cp -R ./generated ./dist/",
    "build": "pnpm prebuild && pnpm schema:build && nest build",
    "build:watch": "pnpm prebuild && pnpm schema:build && nest build --watch",
    "ci:build": "pnpm prebuild && pnpm schema:build && nest build",
    "ci:build-no-engine": "pnpm prebuild && pnpm schema:build-no-engine && nest build",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx,.json,.yaml --fix",
    "test": "jest",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:watch": "jest --watch",
    "type-check": "tsc --pretty --noEmit"
  },
  "dependencies": {
    "@apollo/server": "4.7.5",
    "@gentlepeople/eslint-plugin": "workspace:*",
    "@gentlepeople/taleus-schema": "workspace:*",
    "@google-cloud/functions-framework": "3.3.0",
    "@graphql-tools/utils": "10.0.3",
    "@nestjs/apollo": "12.0.4",
    "@nestjs/axios": "3.0.0",
    "@nestjs/common": "10.0.3",
    "@nestjs/config": "3.0.0",
    "@nestjs/core": "10.0.3",
    "@nestjs/graphql": "12.0.6",
    "@nestjs/jwt": "10.1.0",
    "@nestjs/platform-express": "10.0.3",
    "@nestjs/testing": "10.0.3",
    "apollo-server-core": "3.11.0",
    "axios": "1.7.5",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.0",
    "cors": "2.8.5",
    "dayjs": "1.11.10",
    "express": "4.18.2",
    "firebase": "9.23.0",
    "firebase-admin": "11.10.1",
    "firebase-functions": "4.4.1",
    "graphql": "16.8.1",
    "graphql-query-complexity": "0.12.0",
    "graphql-scalars": "1.22.2",
    "graphql-type-json": "0.3.2",
    "nestjs-pino": "3.3.0",
    "pino-http": "8.3.3",
    "rxjs": "7.8.1",
    "ts-case-convert": "2.0.7"
  },
  "devDependencies": {
    "@gentlepeople/eslint-plugin": "workspace:*",
    "@gentlepeople/tsconfig": "workspace:*",
    "@nestjs/cli": "9.5.0",
    "@types/cors": "2.8.13",
    "@types/cron": "2.0.1",
    "@types/eslint": "8.40.2",
    "@types/express": "4.17.20",
    "@types/graphql-type-json": "0.3.2",
    "@types/jest": "29.5.2",
    "@types/lodash": "4.14.196",
    "@types/lodash.set": "4.3.7",
    "@types/node": "20.4.9",
    "@types/pluralize": "0.0.29",
    "@types/rimraf": "4.0.5",
    "@types/semver": "7.5.0",
    "@types/supertest": "2.0.12",
    "apollo": "2.34.0",
    "eslint": "8.43.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-import-resolver-typescript": "3.5.5",
    "isolate-package": "1.3.3",
    "jest": "29.6.1",
    "jest-html-reporter": "3.10.1",
    "lodash": "4.17.21",
    "prettier": "3.0.0",
    "prisma": "5.9.0",
    "reflect-metadata": "0.1.13",
    "rimraf": "5.0.1",
    "terser-webpack-plugin": "5.3.9",
    "ts-jest": "29.1.1",
    "ts-loader": "9.4.4",
    "ts-node": "10.9.2",
    "tsconfig-paths": "4.2.0",
    "tsconfig-paths-webpack-plugin": "4.1.0",
    "tslib": "2.6.0",
    "type-fest": "4.5.0",
    "typescript": "5.4.2",
    "webpack": "5.88.2",
    "webpack-cli": "5.1.4",
    "webpack-node-externals": "3.0.0"
  },
  "engines": {
    "node": "18",
    "pnpm": "8.10.5"
  },
  "files": [
    "dist",
    ".env"
  ],
  "jest": {
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "coverageReporters": [
      "lcov"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "reporters": [
      "default",
      "../node_modules/jest-html-reporter"
    ],
    "rootDir": "src",
    "testEnvironment": "node",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    }
  }
}
