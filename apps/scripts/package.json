  "devDependencies": {
    "@faker-js/faker": "8.0.0",
    "@gentlepeople/eslint-plugin": "workspace:*",
    "@gentlepeople/taleus-schema": "workspace:*",
    "@gentlepeople/tsconfig": "workspace:*",
    "@types/eslint": "8.40.2",
    "@types/lodash": "4.14.196",
    "@types/node": "20.4.9",
    "@types/prettier": "2.7.3",
    "dayjs": "1.11.10",
    "eslint": "8.43.0",
    "listr2": "7.0.2",
    "chalk": "4.1.2",
    "handlebars": "4.7.8",
    "lodash": "4.17.21",
    "prettier": "3.0.0",
    "prisma": "5.9.0",
    "ts-node": "10.9.2",
    "typescript": "5.4.2"
  },
  "engines": {
    "node": "18.17.1",
    "pnpm": "8.10.5"
